  "scripts": {
    "dev": "webpack --config webpack/webpack.dev.js --env.dev --progress --hide-modules --watch",
    "predev": "npm run clean",
    "prod": "webpack --config webpack/webpack.prod.js --env.prod --progress --hide-modules",
    "preprod": "npm run clean",
    "start": "webpack --config webpack/webpack.server.js --env.server --progress --hide-modules && webpack --config webpack/webpack.prod.js --env.prod --progress --hide-modules",
